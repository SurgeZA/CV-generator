<style>
    * {
        box-sizing: border-box;
    }

    html {
        scroll-behavior: smooth;
    }

    body {
        margin: 0px;
        background: #ffffff;
    }

    #first {
        padding: 10px;
        padding-top: 20px;
        padding-bottom: 110px;
    }

    #first #menu ul li a {
        text-decoration: none;
    }

    .header {
        background: rgb(255, 255, 255);
        color: #f1f1f1;
    }

    .content {
        padding: 16px;
    }

    .sticky {
        top: 0;
        width: 100%;
    }

    #logo {
        float: left;
        margin-left: 30px;
        margin-top: 0px;
        position: absolute;
    }

    #logo img {
        width: 100px;
    }

    #menu {
        font-family: 'Bahnschrift Condensed';
        font-size: 30px;
        background: rgb(255, 255, 255);
        color: rgb(23, 143, 255);
        letter-spacing: 2px;
        margin-right: -10px;
        margin-top: 0px;
        float: right;
        text-decoration: none;
        padding-top: 10px;
        padding-right: 80px;
        padding-left: 36%;
    }

    #menu a {
        color: black;
        text-decoration: none;
    }

    #menu ul {
        list-style: none;

        text-decoration: none;
    }

    #menu ul li {
        display: inline-block;
        margin-left: 40px;
        font-weight: bold;
        text-decoration: none;
    }

    #menu ul li:hover {
        color: dodgerblue;
    }

    #main {
        width: 100%;
        height: 100%;
        margin-top: 50px;
        position: absolute;
        overflow: hidden;
    }




    #box3 {
        overflow: hidden;
    }

    #container {
        width: 100%;
        position: absolute;
        top: -100%;
        animation: anim4 0.1s forwards 0.1s;
    }



    @keyframes anim4 {
        from {
            top: -100%;
        }

        to {
            top: auto;
        }
    }

    #spacing {
        margin-top: 45%;
        /* background-color: rgba(231, 231, 231, 0.575); */
        padding-top: 50px;
        padding-left: 250px;
        padding-bottom: 50px;
        margin-bottom: 20px;
    }

    .column {
        float: left;
        width: 50%;
        padding: 10px;
        height: 400px;
        margin-left: 0px;
    }

    .column h1 {
        font-size: 70px;
        font-family: 'Bahnschrift Condensed';
        color: rgb(73, 182, 255);
        font-weight: 1000;
        letter-spacing: 0.3em;
        line-height: 1;
        margin-bottom: 0.7rem;
        text-align: left;
    }

    .row:after {
        content: "";
        display: table;
        clear: both;
    }

    #column1 {
        overflow: visible;
        width: 40%;
        margin-top: 100px;
        margin-left: -100px;
        margin-right: 100px;
    }

    #column2 {
        width: 50%;
        height: auto;
        padding-left: 20px;
        padding-bottom: 30px;
        border-radius: 10px;
        border-style: solid;
        border-width: 6px;
        border-color: rgb(64, 169, 211);
        box-shadow: 5px 10px 8px 10px #9bdaff65;
    }


    .modal {
        display: none;
        position: fixed;
        z-index: 1;
        padding-top: 100px;
        left: 0;
        top: 0;
        width: 100%;
        height: 100%;
        overflow: auto;
        background-color: rgb(0, 0, 0);
        background-color: rgba(0, 0, 0, 0.4);
    }


    .modal-content {
        background-color: #fefefe;
        margin: auto;
        margin-top: 20px;
        margin-bottom: 50px;
        padding: 20px;
        border: 1px solid #888;
        width: 50%;
    }

    #myBtn {
        color: rgb(0, 0, 0);
        font-size: 20px;
        font-weight: bold;
        background-color: white;
        border-color: dodgerblue;
        border-radius: 20px;
        border-width: 5px;
        margin-top: 10px;
        margin-left: 20px;
        width: 250px;
        height: 50px;

    }

    #myBtn:hover {
        color: rgb(101, 129, 255);
        font-weight: bold;
    }

    .close {
        color: #aaaaaa;
        float: right;
        font-size: 28px;
        font-weight: bold;
    }

    .close:hover,
    .close:focus {
        color: #000;
        text-decoration: none;
        cursor: pointer;
    }

    .modal-content label {
        font-size: 20px;
        color: dodgerblue;
        font-family: 'Bahnschrift Condensed';
    }

    form input {
        width: 60%;
        font-family: 'Bahnschrift Condensed';
        height: 40px;
        margin: 0 20px;
        position: relative;
    }

    form textarea {
        height: 70px;
    }

    input,
    textarea {
        display: block;
        height: 100%;
        width: 100%;
        border: none;
        font-size: 17px;
        border-bottom: 2px solid rgba(0, 0, 0, 0.12);
    }

    textarea {
        resize: none;
        padding-top: 10px;
    }

    .footer {
        margin-top: 150px;
        margin-left: -250;
        margin-bottom: -80px;
        left: 0;
        bottom: 0;
        height: 2px;
        background-color: transparent;
        color: white;

    }







    .mySlides {
        display: none;
    }

    img {
        vertical-align: middle;
    }

    .slideshow-container {
        max-width: 450px;
        position: relative;
        margin: auto;
    }

    .text {
        color: #f2f2f2;
        font-size: 15px;
        padding: 8px 12px;
        position: absolute;
        bottom: 8px;
        width: 100%;
        text-align: center;
    }

    .numbertext {
        color: #f2f2f2;
        font-size: 12px;
        padding: 8px 12px;
        position: absolute;
        top: 0;
    }

    .dot {
        height: 15px;
        width: 15px;
        margin: 0 2px;
        background-color: #bbb;
        border-radius: 50%;
        display: inline-block;
        transition: background-color 0.6s ease;
    }

    .active {
        background-color: #717171;
    }






    .columns {
        float: left;
        width: 50%;
        padding: 10px;
        padding-top: 50px;
        height: 400px;
        margin-left: 0px;
    }

    .columns h1 {
        font-size: 70px;
        font-family: 'Bahnschrift Condensed';
        color: rgb(73, 182, 255);
        font-weight: 1000;
        letter-spacing: 0.3em;
        line-height: 1;
        margin-bottom: 0.7rem;
        text-align: left;
    }

    .new:after {
        content: "";
        display: table;
        clear: both;
    }

    #column3 {
        overflow: visible;
        width: 30%;
        margin-left: -100px;
        margin-right: 100px;
    }

    #column4 {
        width: 50%;
        padding-left: 20px;
        border-radius: 10px;
        border-style: none;
        padding-bottom: 20px;
    }

    #column4 #box3 {
        width: auto;
        height: auto;
        font-family: 'Bahnschrift Condensed';
        color: rgb(0, 0, 0);
        font-size: 50px;
        font-weight: 500;
        position: absolute;
        margin-top: 200px;
        left: 40%;
        overflow: hidden;
        text-decoration: none;
    }

    #box3 a {
        color: black;
        text-decoration: none;
    }

    #text2 {
        position: relative;
        font-size: 65px;
        left: -100%;
        animation: anim3 1s forwards 1s;
        text-shadow: 5px 5px rgba(30, 143, 255, 0.178);
    }

    @keyframes anim3 {
        from {
            left: -100%;
        }

        to {
            left: 0%;
        }
    }

    #footer-info {
        font-family: 'Bahnschrift Condensed';
        text-align: left;
        margin-top: 70px;
        margin-right: 150px;
        display: inline block;
        float: right;
    }

    .new {
        padding-bottom: 100px;
        padding-top: 100px;
    }

    .newnew {
        margin-left: -100px;
        width: 100%;
        max-width: 100%;
        align-items: center;
    }

    #pain {
        padding: 70px;
        width: 100%;
    }

    /* #servicestitle{
    margin-top: 250px;
} */

    #servicestitle h1 {
        font-family: 'Bahnschrift Condensed';
        font-size: 50px;
        text-align: center;
        margin-left: -70px;
    }

    .servicesinfo {
        font-size: 35px;
        font-family: 'Bahnschrift Condensed';
        font-weight: bold;
    }

    .servicesinfo li {
        display: inline-block;
        align-items: center;
        padding-left: 65px;
        margin-top: 20px;
    }




    .flip-card {
        background-color: transparent;
        width: 300px;
        height: 300px;
        perspective: 1000px;
    }

    .flip-card-inner {
        position: relative;
        width: 100%;
        height: 100%;
        text-align: center;
        transition: transform 0.6s;
        transform-style: preserve-3d;
        box-shadow: 0 4px 8px 0 rgba(104, 102, 102, 0.2);
    }

    .flip-card:hover .flip-card-inner {
        transform: rotateY(180deg);
    }

    .flip-card-front,
    .flip-card-back {
        position: absolute;
        width: 100%;
        height: 100%;
        -webkit-backface-visibility: hidden;
        backface-visibility: hidden;
    }

    .flip-card-front {
        background-color: rgb(255, 255, 255);
        color: black;
    }


    .flip-card-back {
        background-color: #000000;
        color: rgb(190, 156, 0);
        transform: rotateY(180deg);
    }

    .flip-card-back h1 {
        font-size: 20px;
        text-align: center;
        padding-left: 50px;
    }

    .flip-card-back p {
        font-size: 20px;
        color: rgb(255, 255, 255);
    }

    .columnz {
        float: right;
        width: 30%;
        padding: 0 10px;
        margin-top: 60px;
    }

    .rowz {
        margin-left: -50px;
    }

    .rowz:after {
        content: "";
        display: table;
        clear: both;
    }

    @media screen and (max-width: 600px) {
        .columnz {
            width: 100%;
            display: block;
            margin-bottom: 20px;
        }
    }

    .flip-card {
        box-shadow: 0 4px 8px 0 rgba(0, 0, 0, 0.2);
        padding: 20px;
        text-align: center;
        background-color: #f1f1f1;
    }

    .cardz {
        margin-left: -100px;
        padding-left: 20px;
    }
</style>

